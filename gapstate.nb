Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"hexGrid", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"wide1_Integer", "?", "Positive"}], ",", 
       RowBox[{"wide2_Integer", "?", "Positive"}], ",", 
       RowBox[{"wide3_Integer", "?", "Positive"}]}], "}"}], ",", 
     RowBox[{"opts", ":", 
      RowBox[{"OptionsPattern", "[", "Graph", "]"}]}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"cells", ",", "edges", ",", "vertices"}], "}"}], ",", 
     RowBox[{
      RowBox[{"cells", "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"CirclePoints", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"Sqrt", "[", "3", "]"}], " ", 
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"1", " ", "j"}], "+", "k", "-", "2"}], ")"}]}], 
                "+", 
                RowBox[{
                 RowBox[{"Sqrt", "[", "3", "]"}], " ", 
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"1", " ", "j"}], "+", "l", "-", "2"}], ")"}]}]}], 
               ",", 
               RowBox[{
                RowBox[{"3", " ", "k"}], "-", "2", "-", 
                RowBox[{"3", " ", "l"}]}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"2", ",", 
               RowBox[{"\[Pi]", "/", "2"}]}], "}"}], ",", "6"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", "wide1"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"k", ",", "wide2"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"l", ",", "wide3"}], "}"}]}], "]"}], ",", "2"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"edges", "=", 
       RowBox[{"Union", "[", 
        RowBox[{"Sort", "/@", 
         RowBox[{"Flatten", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"Partition", "[", 
              RowBox[{"#", ",", "2", ",", "1", ",", "1"}], "]"}], "&"}], "/@",
             "cells"}], ",", "1"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"vertices", "=", 
       RowBox[{"Sort", "[", 
        RowBox[{
         RowBox[{"Union", "[", 
          RowBox[{"Flatten", "[", 
           RowBox[{"edges", ",", "1"}], "]"}], "]"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"#1", "[", 
            RowBox[{"[", "2", "]"}], "]"}], ">", 
           RowBox[{"#2", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], "&"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"IndexGraph", "[", 
       RowBox[{"Graph", "[", 
        RowBox[{
         RowBox[{"UndirectedEdge", "@@@", "edges"}], ",", "opts", ",", 
         RowBox[{"VertexCoordinates", "\[Rule]", 
          RowBox[{"Thread", "[", 
           RowBox[{"vertices", "\[Rule]", "vertices"}], "]"}]}]}], "]"}], 
       "]"}]}]}], "]"}]}], "\[IndentingNewLine]"}]], "Input",
