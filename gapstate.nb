Cell["\<\
grabevec takes the output of Mathematica eigensystem and give you a eigen \
vector corresponding roughly to the value you enter (currently it will give \
out the largest eigenvalue less than 0.1+input)
------------------------------------------------------------------------------\
----------------------\
\>", "Text",
 CellChangeTimes->{{3.829237235762618*^9, 
  3.8292373594649086`*^9}},ExpressionUUID->"c678be2b-3b72-46d1-818d-\
bf14dc0739ae"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"grabevec", "[", 
    RowBox[{"eigensystem_", ",", "eigenval_"}], "]"}], " ", ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", " ", 
    RowBox[{
     RowBox[{"{", "eigenvec", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "0"}], ",", 
        RowBox[{"i", "<", 
         RowBox[{"Length", "[", 
          RowBox[{"eigensystem", "[", 
           RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ",", 
        RowBox[{"i", "++"}], ",", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Abs", "[", 
            RowBox[{
             RowBox[{"eigensystem", "[", 
              RowBox[{"[", 
               RowBox[{"1", ",", "i"}], "]"}], "]"}], "-", "eigenval"}], 
            "]"}], "<", "0.1"}], ",", 
          RowBox[{"eigenvec", " ", "=", 
           RowBox[{"eigensystem", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "i"}], "]"}], "]"}]}]}], "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", "eigenvec"}]}], "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.829237364356646*^9, 3.829237371271448*^9}, 
   3.8292374549298677`*^9},
 CellLabel->
  "In[428]:=",ExpressionUUID->"8cc6340b-f805-47a5-aad7-3da498b583e7"],

Cell["\<\
addtwodeggrees takes a matrix and in-beds it in a matrix two degrees larger, \
this is so we can add the two extra points to finish the cylinder 
------------------------------------------------------------------------------\
----------------------\
\>", "Text",
 CellChangeTimes->{{3.8292374621872272`*^9, 
  3.8292375290611753`*^9}},ExpressionUUID->"7a486c77-e86f-4c7f-9aa9-\
ff214c5af78c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"addtwodegrees", "[", "a_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"newarray", ",", "addones"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"newarray", " ", "=", " ", 
      RowBox[{"SparseArray", "[", 
       RowBox[{
        RowBox[{"Band", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", "1"}], "}"}], "]"}], "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"a", ",", 
          RowBox[{"ConstantArray", "[", 
           RowBox[{"0", ",", 
            RowBox[{"{", 
             RowBox[{"2", ",", "2"}], "}"}]}], "]"}]}], "}"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"newarray", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", "newarray", "]"}], "-", "1"}], ",", 
         RowBox[{
          RowBox[{"Length", "[", "newarray", "]"}], "-", "1"}]}], "]"}], 
       "]"}], " ", "=", " ", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"newarray", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"Length", "[", "newarray", "]"}], ",", 
         RowBox[{"Length", "[", "newarray", "]"}]}], "]"}], "]"}], " ", "=", 
      " ", "1"}], ";", "\[IndentingNewLine]", "newarray"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.833377008517577*^9, 3.8333772045190268`*^9}},
